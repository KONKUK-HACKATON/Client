<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tv_item_matching_list_vertical_nickname"
        android:layout_width="152dp"
        android:layout_height="14dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="19dp"
        android:layout_marginTop="20dp"
        android:textSize="12sp"
        android:fontFamily="@font/pretendardregular"
        android:textFontWeight="700"
        android:text=""
        android:textColor="#366943"
        />
    
    <TextView
        android:id="@+id/tv_item_matching_list_vertical_title"
        android:layout_width="332dp"
        android:layout_height="18dp"
        android:textFontWeight="700"
        android:textSize="15sp"
        android:text="검색 결과가 없습니다."
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="19dp"
        android:fontFamily="@font/pretendardregular"
        android:layout_marginTop="39dp"/>
    
    <TextView
        android:id="@+id/tv_item_matching_list_horizon_contents"
        android:layout_width="332dp"
        android:layout_height="34dp"
        android:textSize="14sp"
        android:textFontWeight="400"
        android:fontFamily="@font/pretendardregular"
        android:textColor="@color/black"
        android:text=""
        android:layout_marginTop="62dp"
        android:layout_marginStart="19dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:maxLines="2"
        android:ellipsize="end"
        />
    
    <TextView
        android:id="@+id/tv_item_matching_list_horizon_reply"
        android:layout_width="wrap_content"
        android:layout_height="14dp"
        android:text=""
        android:textSize="12sp"
        android:textFontWeight="700"
        android:fontFamily="@font/pretendardregular"
        android:textColor="#B3B3B3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="105dp"
        android:layout_marginStart="23dp"/>

    <TextView
        android:id="@+id/tv_temp_matching_list_reply_numbers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/tv_item_matching_list_horizon_reply"
        app:layout_constraintBottom_toBottomOf="@id/tv_item_matching_list_horizon_reply"
        app:layout_constraintStart_toEndOf="@id/tv_item_matching_list_horizon_reply"
        android:layout_marginStart="3dp"
        android:text=""
        android:fontFamily="@font/pretendardregular"
        android:textColor="#B3B3B3"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DADADA"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="141dp"/>



</androidx.constraintlayout.widget.ConstraintLayout>