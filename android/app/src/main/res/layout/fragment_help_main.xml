<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".help.HelpMainFragment">

    <EditText
        android:id="@+id/et_help_main"
        android:layout_width="334dp"
        android:layout_height="39dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="57dp"
        android:layout_marginStart="20dp"
        android:background="@drawable/edit_text_matching_bulletin"
        android:hint="키워드를 입력해 원하는 게시글을 찾아보세요."
        android:textSize="13sp"/>

    <ImageView
        android:id="@+id/iv_help_main_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/search"
        app:layout_constraintTop_toTopOf="@id/et_help_main"
        app:layout_constraintBottom_toBottomOf="@id/et_help_main"
        app:layout_constraintEnd_toEndOf="@id/et_help_main"
        android:layout_marginEnd="12dp"/>

    <View
        android:id="@+id/v_help_main_v1"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        app:layout_constraintTop_toBottomOf="@id/et_help_main"
        android:layout_marginTop="15dp"
        android:background="@color/white"/>


    <TextView
        android:id="@+id/tv_help_main_matching_bulletin"
        android:layout_width="153dp"
        android:layout_height="28dp"
        app:layout_constraintTop_toTopOf="@id/v_help_main_v1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="12dp"
        android:textSize="24sp"
        android:text="학교정보도우미"
        android:textFontWeight="700"
        android:lineHeight="28.64sp"
        android:textColor="@color/black"
        android:fontFamily="@font/pretendardregular"/>

    <TextView
        android:id="@+id/tv_help_main_tv1"
        android:layout_width="293dp"
        android:layout_height="28dp"
        app:layout_constraintBottom_toBottomOf="@id/v_help_main_v1"
        app:layout_constraintTop_toBottomOf="@id/tv_help_main_matching_bulletin"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginBottom="11dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="6dp"
        android:lineHeight="14.32sp"
        android:textSize="12sp"
        android:text="여기저기 흩어져 있던 학교 정보를\n나에게 익숙한 언어로 한 번에 모아볼 수 있어요."
        android:textFontWeight="600"
        android:fontFamily="@font/pretendardregular"
        android:textColor="#366943"/>

    <ImageView
        android:id="@+id/iv_help_main_search_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/search"
        app:layout_constraintTop_toTopOf="@id/et_help_main"
        app:layout_constraintBottom_toBottomOf="@id/et_help_main"
        app:layout_constraintEnd_toEndOf="@id/et_help_main"
        android:layout_marginEnd="12dp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_jirung"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="289dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="131dp"
        />

    <View
        android:id="@+id/fl_info"
        android:layout_width="match_parent"
        android:layout_height="247dp"
        android:background="#F5F5F5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/v_help_main_v1"/>


    <ImageView
